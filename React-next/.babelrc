{
    "presets": ["next/babel"], // next babel 转义器
    "plugins": [ // 引入插件 antd 
      [
        "import", { // 指定参数 antd 
          "libraryName": "antd",
          "libraryDirectory":"lib",
          "style": true
        }
      ],
      [
        "module-resolver", {
          "root": ["./"],
          "alias": {
            "@": "./components",
            "s": "./static/less",
          }
        }
      ]
    ]
  }
  